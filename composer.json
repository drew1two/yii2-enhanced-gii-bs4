{
    "name": "drew1two/yii2-enhanced-gii-bs4",
    "description": "Generate Relational (hasMany, hasOne, belongsTo, & nested) Models & CRUD.",
    "keywords": ["relation", "gii", "yii2", "yii", "enhanced"],
    "license": "BSD-3-Clause",
    "type": "yii2-extension",
    "authors": [
        {
            "name": "Yohanes Candrajaya",
            "email": "moo.tensai@gmail.com"
        },
        {
            "name": "Jiwantoro Ndaru",
            "email": "jiwanndaru@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "yiisoft/yii2": "*",
        "mootensai/yii2-uuid-behavior": "1.0.0",
        "drew1two/yii2-relation-trait": "dev-master",
        "mootensai/yii2-jsblock": "1.0.0",
        "mootensai/yii2-optimistic-lock-validator": "0.0.1",
        "kartik-v/yii2-builder": "v1.6.7",
        "kartik-v/yii2-grid": "v3.3.5",
        "kartik-v/yii2-widgets": "v3.4.1",
        "kartik-v/yii2-export": "v1.4.1",
        "kartik-v/yii2-tabs-x": "v1.2.7",
        "kartik-v/yii2-detail-view": "v1.8.3",
        "kartik-v/yii2-helpers": "v1.3.9"
    },
    "autoload": {
        "psr-4": {
            "drew1two\\enhancedgii\\": ""
        }
    },
    "extra": {
        "bootstrap": "drew1two\\enhancedgii\\Bootstrap"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/drew1two/yii2-relation-trait.git",
            "options": {
                "ssh2": {
                    "username": "git",
                    "privkey_file": "~/.ssh/id_ed25519",
                    "pubkey_file": "~/.ssh/id_ed25519.pub"
                }
            }
        }
    ]
}
